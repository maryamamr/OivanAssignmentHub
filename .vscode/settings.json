{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.dart_tool": true,
    "**/build": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.dart_tool/**": true,
    "**/build/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/build": true,
    "**/.dart_tool": true
  }
}